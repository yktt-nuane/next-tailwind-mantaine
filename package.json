{
  "name": "next-tailwind-mantine",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm run lint:script && npm run lint:style",
    "lint:script": "next lint --dir src --fix",
    "lint:style": "stylelint ./**/*.{css,scss} --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,scss,md,mdx}'",
    "prepare": "husky install",
    "hide": "git secret hide -m",
    "reveal": "git secret reveal -f",
    "test:unit": "vitest run --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "next": "13.0.7",
    "next-axiom": "^0.16.0",
    "react": "18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@mantine/core": "^5.9.4",
    "@mantine/hooks": "^5.9.4",
    "@mantine/next": "^5.9.4",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/react": "^13.4.0",
    "@types/node": "18.11.15",
    "@types/react": "18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-c8": "^0.25.8",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "eslint-config-next": "13.0.7",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.2",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "tailwindcss": "^3.2.4",
    "typescript": "4.9.4",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "^0.25.8",
    "vitest-github-actions-reporter": "^0.9.0"
  }
